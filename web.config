<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="Rewrite to index.html">
          <match url="^(.*)$" />
          <action type="Rewrite" url="inicio.html" />
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>
